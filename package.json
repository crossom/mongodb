	"scripts": {
		"prepare": "husky install",
		"ts-node": "ts-node",
		"test": "jest",
		"test:cov": "jest --coverage",
		"build": "ncc build src/index.ts -o dist -s -m -e reflect-metadata -e @techmmunity/symbiosis",
